# Locations
    #### Notes:
    #### Try implementing this solution to cd:
    #### https://stackoverflow.com/questions/17958567/how-to-make-an-alias-for-a-long-path
export myDropbox=$MYSTORAGE
export myJournal=$MYSTORAGE/my_journal/
export myWork=$MYSTORAGE/work_journal/gtk_journal/
export myProjects=$MYSTORAGE/projects/
export myScripts=$MYSTORAGE/projects/scripts/
export myDownloads=$HOME/Downloads/
export myPictures=$HOME/Pictures/
export myPrograms=$HOME/Programs/
export myDocuments=$HOME/Documents/
export thesisWork=$MYSTORAGE/work_journal/gtk_journal/resources/QE_paper-Shayon/md-masters_thesis
export maintainScripts=$MYSTORAGE/projects/scripts/system-set-up
export myCustomization=$MYSTORAGE/journal_etc/linux-customization

#### temp locations
export myCoursera=$MYSTORAGE/journal_etc/self-study/coursera/
export jobSearch=$MYSTORAGE/journal_etc/files/career/2019/


## These commands were used for QE documents
export myThesisfolder=$MYSTORAGE/latex\ journal/resources/QE_paper-Shayon
# export myQEpresentation=$MYSTORAGE/latex journal/resources/QE_paper-Shayon/QE-presentation conditional pass






